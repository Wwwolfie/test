{
    "version": 2,
    "public": true,
    "builds": [{
      "src": "./index.js",
      "use": "@vercel/node"
    }],
    "routes": [{"handle": "filesystem"},
      {
        "src": "/(?!\/api)(.*)",
        "dest": "index.js"
      },
      {
        "src": "/api/.*",
        "methods": ["POST", "GET"],
        "dest": "/api/index.js"
      }
    ]
  }